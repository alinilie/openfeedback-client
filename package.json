{
  "name": "openfeedback-client",
  "version": "0.1.0",
  "description": "OpenFeedback App using React",
  "main": "server.js",
  "devDependencies": {},
  "scripts": {
    "postinstall": "npm run web:create && npm run assets:install && npm run browserify:info",
    "assets:clear": "node app/clearAssetsSymlinks.js",
    "assets:install": "node app/installAssets.js",
    "web:create": "node app/createWebDirectory.js",
    "browserify:info": "node app/createBrowserifyAliasInfo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alinilie/openfeedback-client.git"
  },
  "keywords": [
    "openfeedback"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alinilie/openfeedback-client/issues"
  },
  "directories": {
    "web": "web",
    "photonSourceDir": "node_modules/photon-power-ui-kit/dist",
    "photonTargetDir": "web/photon",
    "assetsSourceDir": "src/custom_assets",
    "assetsTargetDir": "web/assets"
  },
  "homepage": "https://github.com/alinilie/openfeedback-client#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "colors": "^1.1.2",
    "express": "^4.13.4",
    "photon-power-ui-kit": "git+ssh://git@github.com/eMAGTechLabs/PhotonPowerUIKit.git",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "reactify": "^1.1.1",
    "swig": "^1.4.2"
  }
}
